      <note> SELECT empno,ename,lag(ename,1)  over(ORDER BY empno) 'Adjacent one',
 lead(ename,1)  over(ORDER BY empno) 'Next one'
 FROM emp;


#窗口、分析函数汇总
 SELECT
   percent_rank() over(partition by deptno order by sal desc) as pct_rank,
   cume_dist() over(partition by deptno order by sal desc) as cume_dist,
   rank()over(partition by deptno order by sal desc) as rank,
   row_number() over (ORDER BY sal) AS rnum,
   dense_rank()over(partition by deptno order by sal desc) as dense_rank,
   ntile(4) over (ORDER BY sal) AS ntile,
   lead(ename,1) over(partition by deptno order by sal desc) as lead,
   lag(ename,1) over(partition by deptno order by sal desc) as lag,
   first_value(ename)over(partition by deptno order by sal desc) as first_value,
   last_value(ename)over(partition by deptno order by sal desc) as last_value,
   percentile_cont(0.5)WITHIN GROUP (ORDER BY sal) OVER (PARTITION BY deptno) AS percentile_cont, 
   row_number() over (PARTITION BY deptno ORDER BY sal) AS deptnonum,
   percentile_disc(0.5)WITHIN GROUP (ORDER BY sal) OVER (PARTITION BY deptno) AS percentile_disc, 
   empno,ename,sal,deptno
 FROM emp 
 ORDER BY deptno,sal desc;</note>
